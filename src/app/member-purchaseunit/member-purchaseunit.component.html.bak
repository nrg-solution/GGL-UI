<div *ngIf="div3">
<div>
    <a (click)="homePage()" class="previous">&laquo; Previous</a>
 
</div>
    <div class="col-md-6 col-md-offset-3">
        <alert></alert>

  <h3>Private Tree Form </h3>    
  <form name="form" (ngSubmit)="f.form.valid && privateTree()" #f="ngForm" novalidate>   
  
      <br/>

      

    <div class="form-group" [ngClass]="{ 'has-error': f.submitted && !refmemberID.valid }">
    
      <label for="refmemberID">Own Tree Number *</label>
          <input type="text" class="form-control" placeholder="Eg-GSP10" name="refmemberID" 
        (change)="getTreeName()" [(ngModel)]="model.refmemberID" #refmemberID="ngModel" required />
          <div *ngIf="f.submitted && !refmemberID.valid" class="help-block">GSP Number is required</div>
     
     
         
        </div>

      <div class="form-group" [ngClass]="{ 'has-error': f.submitted && !numberofUnit.valid }">
          <label for="state">Number of Units *</label>            
       <select class="form-control" name ="numberofUnit"  [(ngModel)]="model.numberofUnit" #numberofUnit="ngModel" required>
       <option *ngFor="let numberofUnit of uniteList" value="{{ numberofUnit }}"> {{ numberofUnit }} 
       </option> 
       </select>
                   <div *ngIf="f.submitted && !numberofUnit.valid" class="help-block">Unit is required</div>
       
      </div>


      <div class="form-group">
          <button [disabled]="loading" class="btn btn-primary">Order</button>



      </div>

  
    </form>
    </div>
</div>

